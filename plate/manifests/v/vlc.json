		"Installer for 64bit version":"https://get.videolan.org/vlc/3.0.20/win64/vlc-3.0.20-win64.exe",
		"MSI package for 64bit version":"https://get.videolan.org/vlc/3.0.20/win64/vlc-3.0.20-win64.msi"
	},
	"script":{
		"args":{
			"regex":"/vlc-([\\d.]+)-win[a-z0-9.]+",
			"autoupdate":{
