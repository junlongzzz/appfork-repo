{
	"name":"Apache Tomcat 10",
	"logo":"https://tomcat.apache.org/res/images/tomcat.png",
	"author":"The Apache Software Foundation",
	"description":"Implementation of the Jakarta Servlet, Jakarta Server Pages, Jakarta Expression Language, Jakarta WebSocket, Jakarta Annotations and Jakarta Authentication specifications.",
	"category":"development",
	"platform":"windows",
	"homepage":"https://tomcat.apache.org",
	"version":"10.1.47",
	"url":{
		"apache-tomcat-10.1.47.exe":"https://archive.apache.org/dist/tomcat/tomcat-10/v10.1.47/bin/apache-tomcat-10.1.47.exe",
		"apache-tomcat-10.1.47-windows-x64.zip":"https://archive.apache.org/dist/tomcat/tomcat-10/v10.1.47/bin/apache-tomcat-10.1.47-windows-x64.zip",
		"apache-tomcat-10.1.47-windows-x86.zip":"https://archive.apache.org/dist/tomcat/tomcat-10/v10.1.47/bin/apache-tomcat-10.1.47-windows-x86.zip"
	},
	"script":{
		"args":{
			"url":"https://archive.apache.org/dist/tomcat/tomcat-10/?C=M;O=D",
			"regex":"v(?<version>[\\d.]+)/",
			"autoupdate":{
				"apache-tomcat-${version}.exe":"https://archive.apache.org/dist/tomcat/tomcat-10/v${version}/bin/apache-tomcat-${version}.exe",
				"apache-tomcat-${version}-windows-x64.zip":"https://archive.apache.org/dist/tomcat/tomcat-10/v${version}/bin/apache-tomcat-${version}-windows-x64.zip",
				"apache-tomcat-${version}-windows-x86.zip":"https://archive.apache.org/dist/tomcat/tomcat-10/v${version}/bin/apache-tomcat-${version}-windows-x86.zip"
			}
		},
		"name":"appfork-check-update"
	}
}