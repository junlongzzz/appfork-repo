{
	"author":"Open Source Contributors",
	"name":"NVM for Windows",
	"description":"A node.js version management utility for Windows. Ironically written in Go.",
	"category":"development",
	"platform":"windows",
	"homepage":"https://github.com/coreybutler/nvm-windows",
	"version":"1.1.12",
	"url":{
		"nvm-noinstall.zip":"https://github.com/coreybutler/nvm-windows/releases/download/1.1.12/nvm-noinstall.zip",
		"nvm-setup.exe":"https://github.com/coreybutler/nvm-windows/releases/download/1.1.12/nvm-setup.exe",
		"nvm-setup.zip":"https://github.com/coreybutler/nvm-windows/releases/download/1.1.12/nvm-setup.zip"
	},
	"script":{
		"args":{
			"url":"gh://coreybutler/nvm-windows",
			"autoupdate":{
				"nvm-noinstall.zip":"https://github.com/coreybutler/nvm-windows/releases/download/${version}/nvm-noinstall.zip",
				"nvm-setup.exe":"https://github.com/coreybutler/nvm-windows/releases/download/${version}/nvm-setup.exe",
				"nvm-setup.zip":"https://github.com/coreybutler/nvm-windows/releases/download/${version}/nvm-setup.zip"
			}
		},
		"name":"appfork-check-update"
	}
}